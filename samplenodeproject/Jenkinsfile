pipeline {
    agent any
    stages {
        stage('Checkout'){
        steps{
            git url: 'https://github.com/LCox3236/QA-DockerandJenkins/samplenodeproject', branch: 'main'
            }
        }
        stage('Build'){
        steps{
            sh 'docker build -t nodeapp .'
        }
      }
    }
}
